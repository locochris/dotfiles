Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hintstyle:  hintslight
Xft.hinting:    true
!Xft.hinting: -1
!Xft.hintstyle:  hintnone

!XTerm*font:    xft:Consolas:pixelsize=13:antialias=false 
!XTerm*font:     xft:Consolas for Powerline:pixelsize=13
!XTerm*boldFont: xft:Consolas for Powerline:pixelsize=13

!URxvt*font:     xft:Consolas for Powerline:pixelsize=13
!URxvt*boldFont: xft:Consolas for Powerline:pixelsize=13

XTerm*font:     xft:Droid Sans Mono for Powerline:pixelsize=12
XTerm*boldFont: xft:Droid Sans Mono for Powerline:pixelsize=12

URxvt*font:     xft:Droid Sans Mono for Powerline:pixelsize=12
URxvt*boldFont: xft:Droid Sans Mono for Powerline:pixelsize=12

URxvt.perl-ext-common:  default,tabbed
URxvt*scrollBar:    False 
URxvt*letterSpace: -1

