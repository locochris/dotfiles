Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hintstyle:  hintslight
Xft.hinting:    true
!Xft.hinting: -1
!Xft.hintstyle:  hintnone

XTerm*font:     xft:Ubuntu Mono for Powerline:pixelsize=14
XTerm*font:     xft:Ubuntu Mono for Powerline:pixelsize=14
XTerm*boldFont: xft:Ubuntu Mono for Powerline:pixelsize=14

URxvt*font:     xft:Ubuntu Mono for Powerline:pixelsize=14
URxvt*boldFont: xft:Ubuntu Mono for Powerline:pixelsize=14

URxvt.perl-ext-common:  default,tabbed
URxvt*scrollBar:    False 
URxvt*letterSpace: -1

*background: #000000
*foreground: #eeeeee

! black
*color0: #111111
*color8: #505050

! red
*color1: #b04020
*color9: #f1080e

!green
*color2: #afd700
*color10: #99ff33

!yellow
*color3: #f7cf0a
*color11: #ff9933

!blue
*color4: #3060aa
*color12: #99ccff

!magenta
*color5: #d6156c
*color13: #f05b47

!cyan
*color6: #a0c2de
*color14: #cadaff

!white
*color7: #cccccc
*color15: #ffffff

